<h1>Vereadores</h1>
<section id="vereadores">
  <div class="row">
    <div class="col-md-12">
      <ul>
        <% @vereadores.each do |vereador| %>
          <li><%= link_to image_tag(vereador.avatar_url(:thumb)), vereador %></br><%= vereador.nome %></li>
        <% end %>
      </ul>
    </div>
  </div>
</section>
